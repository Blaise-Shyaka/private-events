<nav>
  <% if session[:name] %>
    <p>You are Log In as: <br> <%= session[:name] %></p>
    <%= link_to  "Log Out", session_path(session[:name]), method: :delete %>
  <% else %>
    <p>You need to Log In or Sign Up</p>
    <%= link_to  "Sign Up", new_user_path%> <br>
    <%= link_to  "Log In", new_session_path%>
  <% end %>
</nav>



<% if @user_created_events %>
  <div class="my-events-cont">
    <h2>Created Events</h2>
    <ul class="my-events">
      <% @user_created_events.each do |me| %>
        <li>
          <%= link_to  me.name, event_path(me.id) %>
        </li>
        <li>
        <%= @user_created_events.title %>
        </li>
      <% end %>
    </ul>
  </div>
<% end %>